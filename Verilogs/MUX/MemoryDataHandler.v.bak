module MemoryDataHandler
#(
    parameter DATA_WIDTH = 32;
)(
    input [DATA_WIDTH -1:0] input_signal, data_read_from_memory,//Read memory
    output [DATA_WIDTH - 1:0] chosen_data_input,//output
    input should_read_from_input//Control Unit
);

    assign chosen_data_input = should_read_from_input ? input_signal : data_read_from_memory;

endmodule
